       [:##right_shift :right_shift :kybs {:alone :!Sclose_bracket} ]
    ]}

    {:des "Cmd/Ctrl -> () : primary thumb keys"
     :rules [
      ;;  [:##left_command :##left_command :apple {:held :left_command :alone :!S9}]
